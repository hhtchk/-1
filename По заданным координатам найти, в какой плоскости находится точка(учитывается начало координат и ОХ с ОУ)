#include <iostream>
#include <string>
using namespace std;
int main()
{
  int x; int y;
  cin>>x;
  cin>>y;
  if (x>0 && y>0){
      cout<<'1';}
  else if (x>0 && y<0){
      cout<<'4';}
  else if (x<0 && y>0){
      cout<<'2';}
  else if (x<0 && y<0){
      cout<<'3';}
  else if (x==0 && y==0){
      cout<<"Origin of coordinates";}
  else if (x==0){
      cout<<"It's on the OX line";}
  else if (y==0){
      cout<<"It's on the OY line";}
}
