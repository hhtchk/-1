#include <iostream>
#include <string>
using namespace std;
int summ(int n){
      int s=0;
      while (n!=0){
        s=s+n%10;
        n=n/10;}
      return s;
    }
int Mult (int n)
{
    int mult=1;
    while (n>0)
      {
        mult*=n%10;
        n/=10;
      }
    return mult;
} 
int main(){
  int b;
  cin>>b;
  cout<<summ(b)<<' '<<Mult(b);
}
